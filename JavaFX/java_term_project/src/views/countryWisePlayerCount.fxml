<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/" prefHeight="506.0" prefWidth="554.0" styleClass="otherPage"
            stylesheets="@../values/styles.css" xmlns="http://javafx.com/javafx/"
            fx:controller="controllers.countryWisePlayerCountController">
    <children>
        <Label layoutX="134.0" layoutY="28.0" styleClass="commonFontForTitles" text="Country-wise Player Count"
               textFill="#eaec52"/>
