                   styleClass="commonFontForTexts">
            <columns>
                <TableColumn id="playerCountTableCountriesColumn" fx:id="playerCountTableCountriesColumn"
                             editable="false" prefWidth="240.79999542236328" text="Countries"/>
                <TableColumn id="playerCountTablePlayersColumn" fx:id="playerCountTablePlayersColumn" editable="false"
