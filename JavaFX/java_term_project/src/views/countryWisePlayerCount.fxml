        </TableView>
        <Button fx:id="backToPlayerSearchButton" layoutX="59.0" layoutY="441.0" mnemonicParsing="false"
                onAction="#handleBackToPlayerSearchButton" styleClass="commonButton" text="Back To Player Search"/>
        <Button fx:id="backToMainButton" layoutX="314.0" layoutY="441.0" mnemonicParsing="false"
                onAction="#handleBackToMainButton" styleClass="commonButton" text="Back To Main Menu"/>
    </children>
