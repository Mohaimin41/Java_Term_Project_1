<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="otherPage" stylesheets="@../values/styles.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="controllers.marketPageController">
   <children>
      <Label alignment="BASELINE_CENTER" layoutX="216.0" layoutY="55.0" styleClass="commonFontForTitles" text="Marketplace" />
      <Button fx:id="buyPlayerButton" alignment="BASELINE_CENTER" layoutX="224.0" layoutY="175.0" mnemonicParsing="false" onAction="#handleBuyPlayerButton" styleClass="commonButton" text="Buy Players" />
      <Button fx:id="sellPlayerButton" alignment="BASELINE_CENTER" layoutX="225.0" layoutY="229.0" mnemonicParsing="false" onAction="#handleSellPlayerButton" styleClass="commonButton" text="Sell Players" />
      <Button fx:id="backToMainButton" alignment="BASELINE_CENTER" layoutX="189.0" layoutY="280.0" mnemonicParsing="false" onAction="#handleBackToMainButton" styleClass="commonButton" text="Back To Main Menu" />
      <Button fx:id="allPlayerPageButton" layoutX="176.0" layoutY="123.0" mnemonicParsing="false" onAction="#handleAllPlayerPageButton" styleClass="commonButton" text="Current Players In Club" />
   </children>
</AnchorPane>
