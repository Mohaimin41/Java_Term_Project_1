<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="351.0" prefWidth="600.0" styleClass="mainPage" stylesheets="@../values/styles.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/" fx:controller="controllers.clubAndCountryInputController">
    <children>
        <Label layoutX="117.0" layoutY="42.0" styleClass="commonFontForTitles" text="Input Club and Country Name" textFill="WHITE" />
        <TextField fx:id="countryBeSearched" layoutX="209.0" layoutY="146.0" prefHeight="36.0" prefWidth="346.0" promptText="Enter country name (&quot;ANY&quot; for all countries)" styleClass="commonFontForLightText" />
        <Button fx:id="searchByCountryButton" layoutX="139.0" layoutY="247.0" mnemonicParsing="false" onAction="#searchByCountry" styleClass="commonButton" text="Submit" />
        <Button fx:id="resetFieldButton" layoutX="260.0" layoutY="247.0" mnemonicParsing="false" onAction="#resetFields" styleClass="commonButton" text="Reset" />
        <Button fx:id="BackToPlayerSearchButton" layoutX="364.0" layoutY="247.0" mnemonicParsing="false" onAction="#backToPlayerSearch" styleClass="commonButton" text="Cancel" />
        <Label layoutX="76.0" layoutY="152.0" styleClass="commonFontForTexts" text="Country Name:" textFill="WHITE" />
    </children>
</AnchorPane>
